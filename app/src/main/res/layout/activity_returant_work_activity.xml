<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent"
                                                   android:background="#394692"
                                                   tools:context=".returant_work_activity">
    
    <ImageButton
            android:id="@+id/cart"
            android:layout_width="140dp"
            android:layout_height="110dp"
            app:srcCompat="@drawable/driver_hire"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintEnd_toStartOf="@+id/res_info" android:layout_marginEnd="8dp"
            android:layout_marginBottom="12dp" app:layout_constraintBottom_toTopOf="@+id/textView"
            app:layout_constraintHorizontal_bias="0.463"/>
    
    <ImageButton
            android:id="@+id/all_orders"
            android:layout_width="140dp"
            android:layout_height="110dp"
            app:srcCompat="@drawable/ic_feedback_black_24dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="36dp"
            android:layout_marginBottom="100dp" app:layout_constraintBottom_toBottomOf="parent"/>
    
    <TextView
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="استئجار سائق"
            android:textColor="@color/white"
            android:textSize="20dp"
            android:layout_marginStart="60dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginBottom="64dp"
            app:layout_constraintBottom_toTopOf="@+id/all_orders"/>
    
    <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="27dp"
            android:text="جميع الطلبات"
            android:textColor="@color/white"
            android:textSize="20dp"
            android:layout_marginStart="68dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="20dp"
            app:layout_constraintTop_toBottomOf="@+id/all_orders"/>
    
    <ImageView
            android:id="@+id/imageView3"
            android:layout_width="395dp"
            android:layout_height="187dp"
            app:srcCompat="@drawable/food_hire"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintVertical_bias="0.0"/>
    
    <ImageButton
            android:id="@+id/logout"
            android:layout_width="140dp"
            android:layout_height="110dp"
            android:background="@drawable/log_out"
            app:layout_constraintStart_toEndOf="@+id/all_orders"
            android:layout_marginStart="44dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintHorizontal_bias="0.68" android:layout_marginTop="48dp"
            app:layout_constraintTop_toBottomOf="@+id/textView24"/>
    
    <TextView
            android:id="@+id/textView12"
            android:layout_width="65dp"
            android:layout_height="39dp"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="60dp"
            android:gravity="center"
            android:text="الخروج"
            android:textColor="@color/white"
            android:textSize="20dp"
            app:layout_constraintBaseline_toBaselineOf="@+id/textView4"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toEndOf="@+id/textView4" />
    
    <ImageButton
            android:id="@+id/res_info"
            android:layout_width="140dp"
            android:layout_height="110dp"
            app:srcCompat="@drawable/ic_info_black_24dp"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="32dp" android:layout_marginBottom="16dp"
            app:layout_constraintBottom_toTopOf="@+id/textView24"/>
    <TextView
            android:text="معلومات المطعم"
            android:layout_width="103dp"
            android:textSize="16dp"
            android:textStyle="bold"
            android:gravity="center"
            android:textColor="@color/white"
            android:layout_height="38dp"
            android:id="@+id/textView24"
            app:layout_constraintBaseline_toBaselineOf="@+id/textView"
            app:layout_constraintStart_toEndOf="@+id/textView" android:layout_marginStart="76dp"
            android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="parent"/>

</androidx.constraintlayout.widget.ConstraintLayout>